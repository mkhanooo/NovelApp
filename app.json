{
  "expo": {
    "name": "novel-app",
    "slug": "novel-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/open-book.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b1c1f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mdkhan.novelapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/open-book.png",
        "backgroundColor": "#1b1c1f"
      },
      "package": "com.mdkhan.novelapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
